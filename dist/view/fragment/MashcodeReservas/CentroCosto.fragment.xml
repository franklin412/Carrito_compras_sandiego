<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="Products"
		items="{localmodel>/CentrosCosto}"
		search=".onSearchCentroCosto"
		confirm=".onConfirmCentroCosto"
		cancel=".onCloseCentroCosto">
		<StandardListItem
			title="{localmodel>CenterCode}"
			description="{localmodel>CenterCode} - {localmodel>CenterName}" />
	</SelectDialog>
</core:FragmentDefinition>